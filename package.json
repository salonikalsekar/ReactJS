{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "![](http://i.imgur.com/5Bqs5zi.png)",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
       "build": "webpack -d && copy \"src/index.html\" \"dist/\" /F /Y && webpack-dev-server --content-base src/ --inline",
